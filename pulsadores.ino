/**/int unidades = 0; int decenas = 0; int pdwon = 31; int pup = 34;void contadorPositivo(){  if(unidades == 9 && decenas == 9){    // RESET    unidades = 0;     decenas = 0;     Serial.print(decenas);    Serial.print(unidades);    Serial.print("/n");  } else {    if(unidades == 9){      unidades = 0;       decenas++; // se incrementa las decenas      Serial.print(decenas);      Serial.print(unidades);      Serial.print("/n");    } else {      unidades++;    }  }  }void contadorNegativo(){  if(unidades == 0 && decenas == 0){    // No se hace nada    Serial.print(decenas);    Serial.print(unidades);    Serial.print("/n");  } else {    if(unidades == 0){      unidades = 0;       decenas--; // se decrementa las decenas      Serial.print(decenas);      Serial.print(unidades);      Serial.print("/n");    } else {      unidades--;    }  }  }void setup() {  Serial.begin(9600);  DDRC = B00000001; // Definimos el PORTC de entrada salvo PC0 (salida)   PORTC= B11111000; // activamos el pull-up interno de las l√≠neas entrada PC7-PC3  Serial.print(decenas);  Serial.print(unidades);  Serial.print("/n");}void loop() {  if(digitalRead(pup) != HIGH){    delay(200);    if(digitalRead(pdwon) != HIGH){      // RESET      unidades = 0;       decenas = 0;       Serial.print(decenas);      Serial.print(unidades);      Serial.print("/n");    } else {      contadorPositivo();    }  }    if(digitalRead(pdwon) != HIGH){    delay(200);    if(digitalRead(pup) != HIGH){      // RESET      unidades = 0;       decenas = 0;       Serial.print(decenas);      Serial.print(unidades);      Serial.print("/n");    } else {      contadorNegativo();    }  }}